{
  "kind": "collectionType",
  "collectionName": "alumnos",
  "info": {
    "singularName": "alumno",
    "pluralName": "alumnos",
    "displayName": "Alumno",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "apellidos": {
      "type": "string"
    },
    "dni": {
      "type": "string",
      "unique": true
    },
    "fecha_nacimiento": {
      "type": "date"
    },
    "direccion": {
      "type": "text"
    },
    "poblacion": {
      "type": "string"
    },
    "cp": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "telefono": {
      "type": "string"
    },
    "fecha_inicio": {
      "type": "date"
    },
    "grado": {
      "type": "string"
    },
    "dojo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::dojo.dojo",
      "inversedBy": "alumnos"
    },
    "grupo": {
      "type": "string"
    },
    "activo": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "fecha_baja": {
      "type": "date"
    },
    "seguro_pagado": {
      "type": "boolean",
      "default": false
    },
    "asistencias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::asistencia.asistencia",
      "mappedBy": "alumno"
    },
    "horas_acumuladas": {
      "type": "decimal",
      "default": 0
    },
    "grado_objetivo": {
      "type": "string"
    },
    "cuenta_usuario": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "es_instructor": {
      "type": "boolean",
      "default": false
    },
    "seminarios": {
      "type": "component",
      "component": "alumnos.seminario",
      "repeatable": true
    },
    "genero": {
      "type": "enumeration",
      "enum": [
        "HOME",
        "DONA"
      ],
      "default": "HOME"
    }
  }
}
